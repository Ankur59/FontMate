{
  "name": "google-font-installer",
  "publisher": "Leo001",
  "displayName": "FontMate",
  "version": "1.0.1",
  "description": "Install Google Fonts directly in VS Code",
  "icon": "assets/images/Logo.png",
  "main": "./out/extension.js",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:googleFontInstaller.installFont"
  ],
  "contributes": {
    "commands": [
      {
        "command": "googleFontInstaller.installFont",
        "title": "Install Google Font"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "vscode-test"
  },
  "keywords": [
    "font",
    "google fonts",
    "typography",
    "Ankur",
    "download fonts",
    "web fonts",
    "custom fonts"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "^22.13.13",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.8.2",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "fs-extra": "^11.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/google-font-installer.git"
  }
}
